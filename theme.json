{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": false
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			}
		},
		"color": {
			"custom": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"gradients": [],
			"link": false,
			"palette": [
				{
				  "color": "inherit",
				  "name": "Inherit",
				  "slug": "inherit"
				},
				{
				  "color": "currentcolor",
				  "name": "Current",
				  "slug": "current"
				},
				{
				  "color": "transparent",
				  "name": "Transparent",
				  "slug": "transparent"
				},
				{
					"color": "#000000",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#2E2E3A",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#BB5D2F",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#DBBCA7",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#DF9151",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#F00",
					"name": "Quinary",
					"slug": "quinary"
				},
				{
					"color": "#F00",
					"name": "Senary",
					"slug": "senary"
				},
				{
					"color": "#F00",
					"name": "Septenary",
					"slug": "septenary"
				},
				{
					"color": "#F00",
					"name": "Octonary",
					"slug": "octonary"
				},
				{
					"color": "#F00",
					"name": "Nonary",
					"slug": "nonary"
				},
				{
					"color": "#F00",
					"name": "Denary",
					"slug": "denary"
				},
				{
					"color": "#595959",
					"name": "Dark gray",
					"slug": "dark-gray"
				},
				{
					"color": "#919191",
					"name": "Medium gray",
					"slug": "medium-gray"
				},
				{
					"color": "#F5F5F5",
					"name": "Light gray",
					"slug": "light-gray"
				}
			]
		},
		"custom": {
			"border-radius": {
				"default": "5px"
			},
			"border-width": {
				"default": "1px"
			},
			"box-shadow": {
				"default": "0px 2px 8px rgba(0, 0, 0, 0.15)"
			},
			"color": {
				"link" : "#0000EE"
			},
			"layout": {
				"content": "800px",
				"wide": "1200px",
				"sidebar": "336px",
				"page": "var(--wp--custom--layout--content)",
				"padding": "16px",
				"block-gap": "16px",
				"block-gap-large": "40px"
			},
			"letter-spacing": {
				"none": "normal"
			},
			"line-height": {
				"default": 1.5
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"blockGap": false,
			"margin": false,
			"padding": false,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"letterSpacing": false,
			"lineHeight": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Sans Serif",
					"slug": "system-sans-serif"
				},
				{
					"fontFamily": "-apple-system-ui-serif, ui-serif, Noto Serif, Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "System Serif",
					"slug": "system-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Extra Small",
					"size": "0.75rem",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
				  "name": "Base",
				  "size": "1rem",
				  "slug": "base"
				},
				{
					"name": "Medium",
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(1.5rem, 2.8vw, 1.75rem)",
					"slug": "x-large"
				},
				{
					"name": "Heading 1",
					"size": "clamp(2.75rem, 5.2vw, 3.25rem)",
					"slug": "heading-1"
				},
				{
					"name": "Heading 2",
					"size": "clamp(2.5rem, 4.8vw, 3rem)",
					"slug": "heading-2"
				},
				{
					"name": "Heading 3",
					"size": "clamp(2.125rem, 4.4vw, 2.75rem)",
					"slug": "heading-3"
				},
				{
					"name": "Heading 4",
					"size": "clamp(2rem, 4vw, 2.5rem)",
					"slug": "heading-4"
				},
				{
					"name": "Heading 5",
					"size": "clamp(1.875rem, 3.6vw, 2.25rem)",
					"slug": "heading-5"
				},
				{
					"name": "Heading 6",
					"size": "clamp(1.75rem, 3.2vw, 2rem)",
					"slug": "heading-6"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline" : {
						"border": {
							"radius": "var(--wp--custom--border-radius--default)",
							"width": "2px"
						},
						"spacing": {
							"padding": {
								"bottom": "8px",
								"left": "13px",
								"right": "13px",
								"top": "8px"
							}
						}
					}
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--layout--block-gap)"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--medium-gray)",
					"text": "var(--wp--preset--color--medium-gray)"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-serif)",
					"lineHeight": "var(--wp--custom--line-height--default)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-serif)",
					"lineHeight": "var(--wp--custom--line-height--default)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic",
					"fontWeight": "normal"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"bottom": "10px",
						"left": "15px",
						"right": "15px",
						"top": "10px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--default)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					},
					"outline": {
						"color": "blue",
						"offset": "3px",
						"style": "solid",
						"width": "3px"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--medium-gray)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--default)"
				}
			},
			"cite": {},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-serif)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--default)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--custom--color--foreground)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--custom--color--foreground)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-sans-serif)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "var(--wp--custom--line-height--default)"
		}
	}
}